{
  "module": "onenote",
  "description": "OneNote MCP 핸들러 테스트 케이스",
  "test_cases": [
    {
      "name": "manage_sections_and_pages (create_section)",
      "enabled": false,
      "tool": "manage_sections_and_pages",
      "arguments": {
        "action": "create_section",
        "user_id": "yhjoe",
        "notebook_id": "1-test-notebook",
        "section_name": "Test Section",
        "section_id": "",
        "page_title": ""
      },
      "expect": {
        "contains": ["success", "true"]
      }
    },
    {
      "name": "manage_sections_and_pages (list_sections)",
      "enabled": true,
      "tool": "manage_sections_and_pages",
      "arguments": {
        "action": "list_sections",
        "user_id": "yhjoe",
        "notebook_id": "",
        "section_name": "",
        "section_id": "",
        "page_title": ""
      },
      "expect": {
        "contains": ["sections", "액세스 토큰이 없습니다"]
      }
    },
    {
      "name": "manage_sections_and_pages (list_pages)",
      "enabled": true,
      "tool": "manage_sections_and_pages",
      "arguments": {
        "action": "list_pages",
        "user_id": "yhjoe",
        "notebook_id": "",
        "section_name": "",
        "section_id": "1-test-section",
        "page_title": ""
      },
      "expect": {
        "contains": ["pages", "액세스 토큰이 없습니다"]
      }
    },
    {
      "name": "manage_page_content (get)",
      "enabled": false,
      "tool": "manage_page_content",
      "arguments": {
        "action": "get",
        "user_id": "yhjoe",
        "page_id": "1-your-page-id",
        "section_id": "",
        "title": "",
        "content": ""
      },
      "expect": {
        "contains": ["success", "content"]
      }
    },
    {
      "name": "manage_page_content (create)",
      "enabled": false,
      "tool": "manage_page_content",
      "arguments": {
        "action": "create",
        "user_id": "yhjoe",
        "page_id": "",
        "section_id": "1-your-section-id",
        "title": "My Test Page",
        "content": "<h1>Test</h1><p>Hello World</p>"
      },
      "expect": {
        "contains": ["success", "true"]
      }
    },
    {
      "name": "manage_page_content (delete)",
      "enabled": false,
      "tool": "manage_page_content",
      "arguments": {
        "action": "delete",
        "user_id": "yhjoe",
        "page_id": "1-your-page-id",
        "section_id": "",
        "title": "",
        "content": ""
      },
      "expect": {
        "contains": ["success", "true"]
      }
    },
    {
      "name": "edit_page",
      "enabled": false,
      "tool": "edit_page",
      "arguments": {
        "user_id": "yhjoe",
        "page_id": "1-your-page-id",
        "content": "<h1>Updated</h1><p>Content updated</p>"
      },
      "expect": {
        "contains": ["success", "true"]
      }
    },
    {
      "name": "db_onenote_update (섹션 저장)",
      "enabled": true,
      "tool": "db_onenote_update",
      "arguments": {
        "user_id": "yhjoe",
        "section_id": "1-test-section",
        "section_name": "Test Section",
        "notebook_id": "1-test-notebook",
